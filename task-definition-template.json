{
    "family": "spacex-frontend-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::842311388019:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "spacex-frontend",
        "image": "REPLACE_ME_LATER",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }